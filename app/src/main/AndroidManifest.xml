<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hali.pro.com.haliyikama">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name="com.activeandroid.app.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!--
     ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data
            android:name="AA_DB_NAME"
            android:value="HaliYikamaLocal.db"></meta-data>
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1"></meta-data>


        <activity android:name=".ServisResources.login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ServisResources.MainLogin"
            android:label="@string/title_activity_main_login" />
        <activity android:name=".ServisResources.AddAccount" />
        <activity android:name=".ServisResources.InformationAccount" />
        <activity android:name=".ServisResources.SearchAccount" />
        <activity android:name=".ServisResources.Print" />
        <activity android:name=".ServisResources.Basket" />
        <activity android:name=".ServisResources.AfterAccepted" />
        <activity android:name=".ServisResources.BeforeAccepted" />
        <activity android:name=".ServisResources.ReadyAccepted"></activity>

    </application>

</manifest>